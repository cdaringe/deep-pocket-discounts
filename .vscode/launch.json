  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "file",
      "program": "${file}",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "compile:all",
      "program": "${workspaceFolder}/scripts/build.js",
      "args": [
        "compile:all"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}
